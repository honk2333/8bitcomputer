library ieee; 
use ieee.std_logic_1164.all;
 use ieee.std_logic_unsigned.all;
 use ieee.std_logic_arith.all; 
 entity buzzer is port(clk :in std_logic; beep:out std_logic);
 end buzzer; architecture behave of buzzer is signal clks:std_logic;
 begin process(clk) variable temp:integer range 0 to 12000; 
begin if(clk'event and clk='1') then if temp=12000 then clks<='1';temp:=0; 
else temp:=temp+1;clks<='0'; 
end if; end if; beep<=clks; end process; end behave;